<h1>Movie details :</h1>
<h2>{{oneMovie.title}}</h2>
<p>Genre : {{oneMovie.genre}}</p>
<p>Plot : {{oneMovie.plot}}</p>
<h3>Cast :</h3>
{{#each oneMovie.cast}}
    <p>Name : {{name}}</p>
    <ul>
        <li>Occupation : {{occupation}}</li>
        <li>Catchphrase : {{catchPhrase}}</li>
    </ul>
{{/each}}
<form action="/movies/{{oneMovie._id}}/delete" method="post">
    <input type="submit" value="Delete movie">
</form>